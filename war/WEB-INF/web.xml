<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Google_mapp</servlet-name>
		<servlet-class>com.feztheforeigner.mapp.google.Google_mappServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Google_mapp</servlet-name>
		<url-pattern>/google_mapp</url-pattern>
	</servlet-mapping>
	
	
	
	<!-- ME -->
	<servlet>
		<servlet-name>Units</servlet-name>
		<servlet-class>com.feztheforeigner.mapp.google.Units</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Units</servlet-name>
		<url-pattern>/units</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
